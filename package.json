{
  "name": "vuesource",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run test && npm run sass && npm run copy:bs && npm run copy:bsv && npm run copy:vtf && npm run build:parts && npm run clean:css && npm run process:html",
    "build:parts": "npm run sass && npm run compile",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "clean:css": "cleancss -o dist/css/main.min.css src/css/bootstrap.css src/css/main.css src/css/bootstrap-vue.css src/css/vuetify.css",
    "compile": "webpack --config config/webpack.config.prod.js",
    "copy:bs": "ncp node_modules/bootstrap/dist/css/bootstrap.css src/css/bootstrap.css",
    "copy:vtf": "ncp node_modules/vuetify/dist/vuetify.css src/css/vuetify.css",
    "copy:bsv": "ncp node_modules/bootstrap-vue/dist/bootstrap-vue.css src/css/bootstrap-vue.css",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "cross-env NODE_ENV=development npm run sass && npm run copy:bs && npm run copy:bsv && npm run copy:vtf && concurrently --kill-others \"webpack-dev-server --hot --inline\" \"node-sass --watch --output ./src/css ./src/sass\"",
    "lint": "tslint src/**/*.ts",
    "process:html": "htmlprocessor src/index.html -o dist/index.html",
    "sass": "node-sass --output ./src/css ./src/sass",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.0-beta.9",
    "cronstrue": "^0.10.10",
    "css-loader": "^0.28.7",
    "font-loader": "^0.1.2",
    "highlight.js": "^9.12.0",
    "moment": "^2.18.0",
    "typescript-collections": "^1.2.5",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "vee-validate": "^2.0.0-rc.21",
    "vue": "~2.4.4",
    "vue-class-component": "~5.0.0",
    "vue-property-decorator": "~4.0.0",
    "vue-router": "~2.7.0",
    "vue-template-compiler": "^2.5.2",
    "vuetable-2": "^1.7.0",
    "vuetify": "^0.16.3",
    "vuex": "^3.0.0",
    "xml-beautifier": "^0.3.0"
  },
  "devDependencies": {
    "@types/mocha": "~2.2.40",
    "@types/node": "~7.0.8",
    "@types/sinon": "~1.16.36",
    "awesome-typescript-loader": "~3.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.0-beta.9",
    "chai": "~3.5.0",
    "chance": "^1.0.12",
    "clean-css-cli": "~4.0.9",
    "compression-webpack-plugin": "~0.3.2",
    "concurrently": "~3.4.0",
    "copy-webpack-plugin": "~4.0.1",
    "cross-env": "~3.2.4",
    "css-loader": "^0.28.7",
    "es6-promise": "~4.1.0",
    "file-loader": "~0.10.1",
    "globby": "~6.1.0",
    "htmlprocessor": "~0.2.4",
    "http-server": "~0.9.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "jquery": "^3.2.1",
    "json-loader": "~0.5.4",
    "karma": "~1.5.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.2",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.0",
    "karma-webpack": "~2.0.3",
    "lodash.merge": "~4.6.0",
    "minimist": "~1.2.0",
    "mocha": "~3.2.0",
    "ncp": "~2.0.0",
    "node-sass": "~4.5.0",
    "opn-cli": "~3.1.0",
    "phantomjs-prebuilt": "~2.1.14",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.1",
    "rimraf": "~2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "~2.1.0",
    "style-loader": "^0.19.0",
    "tslint": "~4.5.1",
    "tslint-loader": "~3.4.3",
    "typescript": "~2.2.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-css-loaders": "^1.0.0",
    "webpack-dev-server": "^2.9.1"
  }
}
